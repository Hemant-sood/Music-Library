<%- include('./partial/header.ejs'); %>


    <p class="fs-1 text-secondary center">
        <%= data.text %>
    </p>

    <br/>
    <div class="container">
        <div class="row">
            <div class="col-sm-8 center">

                <table class="table whiteColor">
                    <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">Artist</th>

                        </tr>
                    </thead>
                    <tbody>

                        <% for(var i = 0; i<data.artists.length; i++)  {   %>

                            <tr>
                                <th scope="row">
                                    <%= (i+1) %>
                                </th>
                                <td>
                                    <a href='/artist/<%= data.artists[i]._id%>' class="noUnderline fs-4">
                                        <%= data.artists[i].name %>
                                    </a>
                                </td>
                            </tr>
                            <% } %>

                    </tbody>
                </table>

            </div>
        </div>
    </div>

    <br/><br/><br/>


    <%- include('./partial/footer.ejs'); %>