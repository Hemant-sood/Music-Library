<%- include('./partial/header.ejs'); %>


    <div class="container">

        <div class="row">
            <div class="col-sm-3">

            </div>
            <div class="col-sm-9">
                <p class="display-5">Recent Songs</p>

                <% for( var i = 0; i< data.recentSongs.length; i++ )  { %>

                    <div class="card" style="width: 18rem;">
                        <!-- <img src="..." class="card-img-top" alt="..."> -->
                        <div class="card-body">
                            <h5 class="card-title">
                                <%= data.recentSongs[i].name %>
                            </h5>
                            <p class="card-text">
                                <%= data.recentSongs[i].artist %>
                            </p>
                            <audio controls="controls" src=<%=data.recentSongs[i].url%> >

                            </audio>

                            <a href="#" class="btn btn-primary">Go somewhere</a>
                        </div>
                    </div>
                    <% } %>

            </div>
        </div>
    </div>


    <%- include('./partial/footer.ejs'); %>